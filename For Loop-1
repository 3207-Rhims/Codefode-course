#include<stdio.h>
int main()
{
    int cout,sum=0,num;//sum= updated value will be stored
    scanf("%d",&num);//10
    for(cout=1; cout<10; cout++)
    {
       sum+=cout; 
       //sum=sum+cout
       // sum = 0 + 1 = 1
       // sum = 1 + 2 = 3
       // sum=  3 + 3 = 6
       // sum = 6 + 4 = 10
       // sum = 10 + 5 = 15
       // sum= 15+ 6 = 21
       // sum=21 + 7 = 28
       // sum = 28 + 8 = 36
       // sum = 36 + 9 = 45
    }
   printf("%d",sum);

 return 0;
 }

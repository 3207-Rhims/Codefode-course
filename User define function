#include<stdio.h>
int addnumbers(int x , int y);

int main(){

int i , j,k, s;
scanf("%d %d",&i , &j); // i= 5 j=6

s = addnumbers(i,j,k); // 5 , 6   =11

printf("%d",s);

return 0;

}

int addnumbers(int a , int b ,int c) // a= 5 , 6
{
    int sum;
    sum = a+b+c;   // 5+6 = 11
    return sum;  // 11
}
